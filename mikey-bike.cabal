name:               mikey-bike
version:            0.1.0.0
build-type:         Simple
cabal-version:      >= 1.10

executable site
  main-is:          site.hs
  build-depends:    base == 4.*
                  , hakyll == 4.12.*
                  , time >=1.6
                  , time-locale-compat >=0.1
                  , hxt >=9.3
                  , filepath >=1.4
                  , hfsevents
  ghc-options:      -threaded
  default-language: Haskell2010
